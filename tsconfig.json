{
	"compilerOptions": {
	  "outDir": "./deploy",
	  "module": "Node16",
	  "target": "ES2017",
	  "sourceMap": true,
	  "noImplicitAny": true,
	  "preserveConstEnums": true,
	  "allowSyntheticDefaultImports": true,
	  "strictNullChecks": true,
	  "allowJs": true,
	  "experimentalDecorators": true,
	  "emitDecoratorMetadata": true,
	  "moduleResolution": "node",
		  "skipLibCheck": true
	}
}