{
  "name": "typescript_dependencies",
  "version": "1.0.0",
  "description": "myapp API",
  "scripts": {
    "dev": "nodemon --exec ts-node ./server",
    "clean": "rm -rf node_modules",
    "start-dev": "webpack-dev-server --open",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest --config=jest.config.js test",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts)\"",
    "lint": "npx eslint \"src/**\" \"__tests__/**\"",
    "lint-fix": "npx eslint \"src/**\" \"__tests__/**\" --fix",
    "postbuild": "echo \"moving node_modules to deploy\" && cp -r node_modules deploy",
    "ts": "ts-node --project tsconfig.e2e.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "aws-lambda": "^1.0.7",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.56.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.6.0",
    "http-errors": "^2.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "source-map": "^0.7.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.18.3",
    "pg": "^8.11.3",
    "prettier": "^3.2.5",
    "typeorm": "^0.3.20",
    "winston": "^3.11.0"
  }
}
